            $(".product-slider").slick({
                dots: false,
                slidesToShow: 3,
                centerMode: true,
                centerPadding: "50px",
                arrows: false,
                slidesToScroll: 1,
                responsive: [
                    {
                        breakpoint: 567,
                        settings: {
                            slidesToShow: 3, centerPadding: "25px",
                        },
                    },
                    {
                        breakpoint: 475,
                        settings: {
                            slidesToShow: 2, centerPadding: "25px",
                        },
                    },
                    {
                        breakpoint: 375,
                        settings: {
                            slidesToShow: 1,
                            centerPadding: "120px"
                        },
                    }
                ],
            });
        });
    </script>
